  "description": "gtforge server",
  "scripts": {
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 4000 --host 0.0.0.0",
    "build": "webpack --progress --colors",
    "build-stage": "NODE_ENV=stage webpack --progress --colors",
    "build-production": "NODE_ENV=production webpack --progress --colors",
    "test": "karma start --single-run --no-auto-watch karma.config.js",
    "stop": "pkill -f webpack"
  },
